package com.kpm.reporter.excel{		import flash.xml.XMLDocument;		/**	 *Clase para manejar los distintos tipos de profile: Teacher, Kid. 	 * 	 */		public class Profile {		/**		 * Método estático que retorna el tipo de un profile.		 * @param xml XML del profile que se pretende parsear.		 * 		 **/		public static function getType(xml:XML):String {						if(xml.hasOwnProperty("FIRST_NAME")){				return "KID";			}			else{								return "OTHER";			}		}	}}